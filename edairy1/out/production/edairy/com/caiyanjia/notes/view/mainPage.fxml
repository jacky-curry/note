<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.caiyanjia.notes.controller.meauController">
   <children>
        <TableView fx:id="myTable" layoutX="104.0" layoutY="88.0" prefHeight="412.0" prefWidth="494.0">
            <columns>
                <TableColumn fx:id="title" prefWidth="102.0" text="title" />
                <TableColumn fx:id="author" prefWidth="86.0" text="author" />
                <TableColumn fx:id="post_date" prefWidth="75.0" text="post date" />
                <TableColumn fx:id="like" prefWidth="59.0" text="like" />
                <TableColumn fx:id="edit" prefWidth="102.0" text="edit" />
            <TableColumn fx:id="like_button" prefWidth="69.0" text="点赞" />
            </columns>
        </TableView>
       <VBox layoutX="1.0" layoutY="3.0" prefHeight="400.0" prefWidth="94.0" spacing="5.0">
          <children>
              <Button fx:id="createNote" mnemonicParsing="false" onAction="#gotoNewNote" prefHeight="73.0" prefWidth="101.0" text="新建笔记" />
              <Button fx:id="MyNote" mnemonicParsing="false" onAction="#gotoMyNote" prefHeight="47.0" prefWidth="101.0" text="我的笔记" />
              <Button fx:id="myInformation" mnemonicParsing="false" onAction="#myInformationShow" prefHeight="47.0" prefWidth="101.0" text="我的信息" />
              <Button mnemonicParsing="false" onAction="#notice_show" prefHeight="47.0" prefWidth="101.0" text="公告" />
              <TextField fx:id="Find" />
            <Label fx:id="Tip" prefHeight="15.0" prefWidth="98.0" />
            <Button fx:id="search_button" mnemonicParsing="false" onAction="#searchNote" prefHeight="23.0" prefWidth="94.0" text="搜索" />
            <Button fx:id="refresh_button" mnemonicParsing="false" onAction="#refresh" prefHeight="23.0" prefWidth="92.0" text="刷新" />
          </children>
       </VBox>
       <Label contentDisplay="CENTER" layoutX="123.0" layoutY="3.0" prefHeight="23.0" prefWidth="477.0" text="笔记空间">
         <font>
            <Font name="Yu Gothic UI Bold" size="36.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <Button layoutX="6.0" layoutY="430.0" mnemonicParsing="false" onAction="#exit" prefHeight="40.0" prefWidth="77.0" text="注销" />
      <Label fx:id="black" layoutX="342.0" layoutY="11.0" prefHeight="40.0" prefWidth="198.0" textFill="#f50000">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
